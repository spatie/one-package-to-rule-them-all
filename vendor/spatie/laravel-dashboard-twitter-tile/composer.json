{
    "name": "spatie/laravel-dashboard-twitter-tile",
    "description": "A Twitter tile for Laravel Dashboard",
    "keywords": [
        "spatie",
        "laravel-dashboard-twitter-tile"
    ],
    "homepage": "https://github.com/spatie/laravel-dashboard-twitter-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^7.4",
        "spatie/laravel-dashboard": "^2.0",
        "spatie/laravel-twitter-streaming-api": "^1.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\TwitterTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\TwitterTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\TwitterTile\\TwitterTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
