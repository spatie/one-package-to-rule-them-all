{
    "name": "spatie/laravel-enum",
    "description": "Laravel Enum support",
    "keywords": [
        "spatie",
        "enum",
        "laravel",
        "laravel-enum"
    ],
    "homepage": "https://github.com/spatie/laravel-enum",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Roose",
            "email": "brent@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Tom Witkowski",
            "email": "dev@gummibeer.de",
            "homepage": "https://gummibeer.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "illuminate/console": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/support": "^8.0",
        "spatie/enum": "^3.9"
    },
    "conflict": {
        "bensampo/laravel-enum": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.0"
    },
    "suggest": {
        "fzaninotto/faker": "^1.9.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Enum\\Laravel\\EnumServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Enum\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Enum\\Laravel\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "psalm": "vendor/bin/psalm -c psalm.xml",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html=build"
    }
}
