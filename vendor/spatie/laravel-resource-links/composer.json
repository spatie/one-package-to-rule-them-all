{
    "name" : "spatie/laravel-resource-links",
    "description" : "Add links to Laravel API resources",
    "keywords" : [
        "spatie",
        "laravel-resource-links"
    ],
    "homepage" : "https://github.com/spatie/laravel-resource-links",
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Ruben Van Assche",
            "email" : "ruben@spatie.be",
            "homepage" : "https://spatie.be",
            "role" : "Developer"
        }
    ],
    "require" : {
        "php" : "^7.2|^8.0",
        "illuminate/database" : "~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/routing" : "~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev" : {
        "orchestra/testbench" : "~3.8.0|^4.0|^5.0|^6.0",
        "phpunit/phpunit" : "^8.0|^9.0"
    },
    "autoload" : {
        "psr-4" : {
            "Spatie\\ResourceLinks\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Spatie\\ResourceLinks\\Tests\\" : "tests"
        }
    },
    "scripts" : {
        "test" : "vendor/bin/phpunit",
        "test-coverage" : "vendor/bin/phpunit --coverage-html coverage"
    },
    "config" : {
        "sort-packages" : true
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "Spatie\\ResourceLinks\\ResourceLinksServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
